{
    "name": "rafaelogic/writr",
    "description": "A comprehensive Notion-like editor for Laravel using Editor.js with advanced features",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "editor", "notion", "wysiwyg", "editorjs", "blade", "component"],
    "authors": [
        {
            "name": "Rafael",
            "email": "rafael@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.4",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Rafaelogic\\Writr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rafaelogic\\Writr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rafaelogic\\Writr\\WritrServiceProvider"
            ],
            "aliases": {
                "Writr": "Rafaelogic\\Writr\\Facades\\Writr"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    }
}
